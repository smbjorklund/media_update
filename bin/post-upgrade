#!/bin/bash

set -x

REL=4.2

if git tag | grep -q R$REL; then
    echo "Disabled after $REL has been released";
    exit 1
fi

# Migrate
bin/site-drush migrate-import --verbose Area --strict=0 --serial=3899846132506877758,7117914931746643279

exit  # for now
bin/site-drush migrate-import --verbose Area --strict=0 --area_subset=uib_areas_subset_utdanning.txt
bin/site-drush migrate-import --verbose NavPage --strict=0 --area_subset=uib_areas_subset_utdanning.txt
bin/site-drush migrate-import --verbose InfoPage --strict=0 --area_subset=uib_areas_subset_utdanning.txt
bin/site-drush migrate-import --verbose News --strict=0 --area_subset=uib_areas_subset_utdanning.txt
bin/site-drush migrate-import --verbose Event --strict=0 --area_subset=uib_areas_subset_utdanning.txt
bin/site-drush migrate-import --verbose Testimonial --strict=0 --area_subset=uib_areas_subset_utdanning.txt

# Build area menu for migrated content
bin/site-drush uib-migrate-build-menu --verbose

# Split Area menu for newly migrated content
bin/site-drush uib-area-menu-chk
bin/site-drush uib-area-split-menu --verbose
bin/site-drush uib-area-menu-to-relation

# Make sure the cache is really blenda-clean
bin/site-drush cc all
